<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="4000"
        motion:motionInterpolator="easeInOut">

        <OnClick
            motion:clickAction="toggle"
            motion:targetId="@id/main_image1"/>

        <KeyFrameSet>

            <KeyPosition
                motion:framePosition="30"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image1"
                motion:pathMotionArc="flip"
                motion:percentX="0.25"
                motion:percentY="0.65"/>

            <KeyPosition
                motion:framePosition="46"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image1"
                motion:pathMotionArc="flip"
                motion:percentX="0"
                motion:percentY="0.5"/>

            <KeyPosition
                motion:framePosition="62"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image1"
                motion:pathMotionArc="flip"
                motion:percentX="0.5"
                motion:percentY="0.25"/>

            <KeyPosition
                motion:framePosition="78"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image1"
                motion:pathMotionArc="flip"
                motion:percentX="1"
                motion:percentY="0.5"/>

            <KeyPosition
                motion:framePosition="94"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image1"
                motion:pathMotionArc="flip"
                motion:percentX="0.5"
                motion:percentY="0.75"/>

        </KeyFrameSet>

        <KeyFrameSet>

            <KeyPosition
                motion:framePosition="26"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image2"
                motion:pathMotionArc="flip"
                motion:percentX="0.25"
                motion:percentY="0.65"/>

            <KeyPosition
                motion:framePosition="42"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image2"
                motion:pathMotionArc="flip"
                motion:percentX="0"
                motion:percentY="0.5"/>

            <KeyPosition
                motion:framePosition="58"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image2"
                motion:pathMotionArc="flip"
                motion:percentX="0.5"
                motion:percentY="0.25"/>

            <KeyPosition
                motion:framePosition="74"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image2"
                motion:pathMotionArc="flip"
                motion:percentX="1"
                motion:percentY="0.5"/>

            <KeyPosition
                motion:framePosition="90"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image2"
                motion:pathMotionArc="flip"
                motion:percentX="0.5"
                motion:percentY="0.75"/>

        </KeyFrameSet>

        <KeyFrameSet>

            <KeyPosition
                motion:framePosition="22"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image3"
                motion:pathMotionArc="flip"
                motion:percentX="0.25"
                motion:percentY="0.65"/>

            <KeyPosition
                motion:framePosition="38"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image3"
                motion:pathMotionArc="flip"
                motion:percentX="0"
                motion:percentY="0.5"/>

            <KeyPosition
                motion:framePosition="54"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image3"
                motion:pathMotionArc="flip"
                motion:percentX="0.5"
                motion:percentY="0.25"/>

            <KeyPosition
                motion:framePosition="70"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image3"
                motion:pathMotionArc="flip"
                motion:percentX="1"
                motion:percentY="0.5"/>

            <KeyPosition
                motion:framePosition="86"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image3"
                motion:pathMotionArc="flip"
                motion:percentX="0.5"
                motion:percentY="0.75"/>

        </KeyFrameSet>

        <KeyFrameSet>

            <KeyPosition
                motion:framePosition="18"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image4"
                motion:pathMotionArc="flip"
                motion:percentX="0.25"
                motion:percentY="0.65"/>

            <KeyPosition
                motion:framePosition="34"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image4"
                motion:pathMotionArc="flip"
                motion:percentX="0"
                motion:percentY="0.5"/>

            <KeyPosition
                motion:framePosition="50"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image4"
                motion:pathMotionArc="flip"
                motion:percentX="0.5"
                motion:percentY="0.25"/>

            <KeyPosition
                motion:framePosition="66"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image4"
                motion:pathMotionArc="flip"
                motion:percentX="1"
                motion:percentY="0.5"/>

            <KeyPosition
                motion:framePosition="82"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/main_image4"
                motion:pathMotionArc="flip"
                motion:percentX="0.5"
                motion:percentY="0.75"/>

        </KeyFrameSet>

    </Transition>

    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:id="@+id/main_image1"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:drawPath="path"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:pathMotionArc="startVertical"/>

        <Constraint
            android:id="@+id/main_image2"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:drawPath="path"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:pathMotionArc="startVertical"/>

        <Constraint
            android:id="@+id/main_image3"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:drawPath="path"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:pathMotionArc="startVertical"/>

        <Constraint
            android:id="@+id/main_image4"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:drawPath="path"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:pathMotionArc="startVertical"/>

    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            android:id="@+id/main_image1"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toStartOf="@id/main_image2"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintVertical_chainStyle="spread_inside"/>

        <Constraint
            android:id="@+id/main_image2"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toStartOf="@id/main_image3"
            motion:layout_constraintStart_toEndOf="@id/main_image1"/>

        <Constraint
            android:id="@+id/main_image3"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toStartOf="@id/main_image4"
            motion:layout_constraintStart_toEndOf="@id/main_image2"/>

        <Constraint
            android:id="@+id/main_image4"
            android:layout_width="60dp"
            android:layout_height="60dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/main_image3"/>

    </ConstraintSet>

</MotionScene>